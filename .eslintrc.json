{
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node": true
    },
    "rules": {
        "indent": [2, 2],
        "max-len": ["error", 80],
        "no-console": "off",
        "prefer-destructuring": "off",
        "prefer-const": "off"
    }
}